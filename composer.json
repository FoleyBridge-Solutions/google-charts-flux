{
    "name": "foleybridgesolutions/google-charts-flux",
    "description": "Google Charts integration for Laravel Livewire and Flux UI with composable Blade components",
    "version": "1.0.0",
    "keywords": ["laravel", "livewire", "flux-ui", "google-charts", "charts", "visualization", "alpine"],
    "license": "MIT",
    "authors": [
        {
            "name": "FoleyBridge Solutions",
            "email": "dev@foleybridgesolutions.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/view": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "phpunit/phpunit": "^10.0",
        "laravel/pint": "^1.27"
    },
    "autoload": {
        "psr-4": {
            "FoleyBridgeSolutions\\GoogleChartsFlux\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "FoleyBridgeSolutions\\GoogleChartsFlux\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "FoleyBridgeSolutions\\GoogleChartsFlux\\GoogleChartsFluxServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
